package com.tyss.capgemini.threads;

public class PrintingMethodClass1 extends Thread{

	@Override
	public void run() {
        Thread.sleep(500);
		for(int i = 1; i < 10;i++)
			System.out.println("printing values: " + i);
	}
	}
	
	
