package com.tyss.capgemini.threads;

public class Class2 implements Runnable{
	@Override
	public void run() {
		for(int i=21;i<=30;i++)
		{
			Thread t1=new Thread();
			try {
				t1.sleep(500);
			} catch (InterruptedException e) {
				
				e.printStackTrace();
			}
		System.out.println(i);
		}
	}

}
