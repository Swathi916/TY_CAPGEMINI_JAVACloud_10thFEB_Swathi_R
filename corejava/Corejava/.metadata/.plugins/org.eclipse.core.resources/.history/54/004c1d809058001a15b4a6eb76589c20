package com.tyss.capgemini.methods;

public class VaragsInputExample {
 public int add(int ...input)//can create array like this also ...name
 {
	 int sum=0;
	 for(int i=0; i < input.length; i++)
	 {
		 sum+= input[i];//compound expression;
	 }
	 return sum;
	 
 }
 public static void main(String[] args) {
	System.out.println(new VaragsInputExample().add(1,2,3,5,7,8,9));
}
}