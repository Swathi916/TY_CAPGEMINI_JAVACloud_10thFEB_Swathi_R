package com.tyss.capgemini.threads;
// developing app
public class PrintingApplication {
	synchronized public void printingDocuments(String document, int numofCopies) {
		for(int i=1;i<=numofCopies;i++)
		{
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		System.out.println(">>Printing" + document+ "#" +i );
		}
	}

}
/** for better order go for synchronized as well as join
 * if we use only sync then we get doc in proper order but will not get as expected
 * */
 */